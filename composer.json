{
    "name": "knutschsoft/swapp",
    "license": "MIT",
    "type": "project",
    "description": "streetworker app",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "platform": {
            "php": "7.4"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "api-platform/core": "2.5.9",
        "composer/package-versions-deprecated": "1.11.99.1",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^1.11",
        "doctrine/doctrine-migrations-bundle": "^2.0",
        "doctrine/orm": "^2.6",
        "dunglas/doctrine-json-odm": "^0.1.3",
        "easycorp/easy-log-handler": "^1.0",
        "easycorp/easyadmin-bundle": "~2.3",
        "gesdinet/jwt-refresh-token-bundle": "^0.9.1",
        "league/csv": "^9.5",
        "lexik/jwt-authentication-bundle": "^2.8",
        "nelmio/cors-bundle": "^2.1",
        "nelmio/js-logger-bundle": "^1.6",
        "ninsuo/symfony-collection": "^2.1",
        "phpdocumentor/reflection-docblock": "^5.1",
        "ramsey/uuid": "^4.1",
        "rotexsoft/versatile-collections": "^2.0",
        "sensio/framework-extra-bundle": "^5.2",
        "stof/doctrine-extensions-bundle": "^1.3",
        "symfony/asset": "4.4.*",
        "symfony/expression-language": "4.4.*",
        "symfony/flex": "^1.4",
        "symfony/messenger": "4.4.*",
        "symfony/monolog-bundle": "~3.3",
        "symfony/property-access": "4.4.*",
        "symfony/property-info": "4.4.*",
        "symfony/proxy-manager-bridge": "4.4.*",
        "symfony/security-bundle": "4.4.*",
        "symfony/serializer": "4.4.*",
        "symfony/twig-bundle": "4.4.*",
        "symfony/validator": "4.4.*",
        "symfony/webpack-encore-bundle": "^1.6",
        "twig/extensions": "^1.5",
        "vich/uploader-bundle": "^1.10"
    },
    "require-dev": {
        "ext-zip": "*",
        "behat/mink": "^1.7",
        "captainhook/captainhook": "^5.0",
        "dbrekelmans/bdi": "^0.3.0",
        "elao/web-profiler-extra-bundle": "^2.3",
        "friends-of-behat/symfony-extension": "^2.1",
        "genesis/behat-fail-aid": "^3.5",
        "hautelook/alice-bundle": "^2.5",
        "phpstan/phpstan": "^0.11.1",
        "phpstan/phpstan-webmozart-assert": "^0.11.2",
        "phpunit/phpunit": "8.5.2",
        "projektmotor/symfony-coding-standard": "^1.0.3",
        "robertfausk/behat-panther-extension": "^1.0",
        "robertfausk/behat-screenshot-image-driver-url": "^1.0",
        "soyuka/contexts": "^3.3",
        "symfony/browser-kit": "4.4.*",
        "symfony/css-selector": "4.4.*",
        "symfony/dotenv": "^4.4",
        "symfony/maker-bundle": "^1.13",
        "symfony/phpunit-bridge": "^4.4",
        "symfony/profiler-pack": "^1.0",
        "thibautselingue/local-php-security-checker-installer": "^1.0"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "scripts": {
        "cbf": "vendor/bin/phpcbf --colors src tests || true",
        "cs": "vendor/bin/phpcs -s src tests || true",
        "behat": "vendor/bin/behat --colors || true",
        "unit": "vendor/bin/phpunit --colors || true",
        "stan": "vendor/bin/phpstan analyse --no-progress --ansi || true",
        "security": "vendor/bin/local-php-security-checker || true",
        "compile-test": [
            "bin/console doctrine:database:create --env=test --no-interaction",
            "bin/console doctrine:schema:update --dump-sql --env=test --ansi",
            "bin/console doctrine:schema:update --force --env=test --ansi",
            "chmod +x var/cache/test/test.db",
            "chmod 777 var/cache/test/test.db"
        ],
        "qa": [
            "@cbf",
            "@cs",
            "@stan",
            "@unit",
            "@behat",
            "@security"
        ],
        "qa-full": [
            "bin/console doctrine:schema:update --env=test --force",
            "yarn encore production --colors",
            "@qa"
        ],
        "qa-ci": [
            "vendor/bin/phpcs --colors src tests",
            "vendor/bin/phpstan analyse --no-progress --ansi",
            "vendor/bin/phpunit --colors",
            "vendor/bin/behat --colors",
            "vendor/bin/local-php-security-checker"
        ],
        "create-jwt": [
            "mkdir -p config/jwt",
            "chmod -R 777 config/jwt",
            "openssl genrsa -out config/jwt/private.pem 4096",
            "openssl rsa -pubout -in config/jwt/private.pem -out config/jwt/public.pem"
        ],
        "database-init": [
            "php bin/console doctrine:database:drop --no-interaction --force --if-exists",
            "php bin/console doctrine:database:create --if-not-exists",
            "php bin/console doctrine:migrations:migrate --no-interaction",
            "php bin/console hautelook_alice:doctrine:fixtures:load --no-interaction"
        ],
        "database-prototype": [
            "php bin/console doctrine:generate:entities AppBundle",
            "php bin/console doctrine:migrations:diff --no-interaction",
            "php bin/console doctrine:migrations:migrate --no-interaction"
        ],
        "database-migrations-diff": [
            "php bin/console doctrine:database:drop --no-interaction --force --if-exists",
            "php bin/console doctrine:database:create",
            "php bin/console doctrine:migrations:diff"
        ],
        "post-install-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget",
            "phpcs --config-set installed_paths vendor/projektmotor/symfony-coding-standard/ProjektMOTORCodingStandard/",
            "local-php-security-checker-installer"
        ],
        "post-update-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget",
            "phpcs --config-set installed_paths vendor/projektmotor/symfony-coding-standard/ProjektMOTORCodingStandard/"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "symfony": {
            "allow-contrib": true,
            "require": "4.4.*"
        }
    }
}
