{% extends '::base.html.twig' %}

{% block title %}{{ 'tag.list.title'|trans }}{% endblock %}
{% block content %}
    {% for flashMessage in app.session.flashbag.get('notice') %}
        <div class="flash-notice">
            {{ flashMessage }}
        </div>
    {% endfor %}
    <div class="swapp-headline-container">
        <h1>
            {{ 'tag.list.headline'|trans }}
        </h1>
    </div>
    <div class="swapp-password-change-container">
        <div class="mb1">
            <a href="{{ path('walk_home_screen') }}" class="btn btn-primary">{{ 'global.navigation.back'|trans }}</a>
            <a href="{{ path('tag_create') }}" class="btn btn-primary">{{ 'tag.create.title'|trans }}</a>
        </div>
    </div>
    {{ include ('@App/Tag/tagOverview.html.twig', { 'tags': tags }, with_context = false) }}
{% endblock %}
